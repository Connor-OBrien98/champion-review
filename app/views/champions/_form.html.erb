<%= simple_form_for @champion, :html => { :multipart => true } do |f| %>
    <%= select_tag(:category_id, options_for_select(@categories), :prompt => "Select a category") %>
    <%= f.file_field :champion_img %>
    <%= f.input :name, label: "Champion Name" %>
    <%= f.input :alias %>
    <%= f.button :submit %>
<% end %>